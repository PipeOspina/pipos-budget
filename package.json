{
  "name": "pipos-budget",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --open",
    "build:dev": "tsc && vite build -m development",
    "build": "tsc && vite build",
    "gcp-build": "if test \"$NODE_ENV\" = \"production\" ; then npm run build ; else npm run build:dev ; fi ",
    "preview": "vite preview",
    "start": "node server.js",
    "lint": "npx eslint src",
    "lint:fix": "npm run lint -- --fix",
    "prettier": "npx prettier src --check",
    "prettier:fix": "npm run prettier -- --write",
    "format": "npm run prettier:fix && npm run lint:fix",
    "prepare": "husky install",
    "env:dev": "dotenv -e .env.development cross-var",
    "env:prod": "dotenv -e .env.production cross-var",
    "deploy:dev": "npm run env:dev \"gcloud app deploy app.dev.yaml --project=%GCP_PROJECT_ID% --quiet\"",
    "deploy": "npm run env:prod \"gcloud app deploy --project=%GCP_PROJECT_ID% --quiet\""
  },
  "dependencies": {
    "@emotion/react": "^11.10.4",
    "@emotion/styled": "^11.10.4",
    "@mui/icons-material": "^5.10.3",
    "@mui/material": "^5.10.4",
    "express": "^4.18.1",
    "firebase": "^9.9.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "6"
  },
  "devDependencies": {
    "@types/node": "^18.7.15",
    "@types/react": "^18.0.17",
    "@types/react-dom": "^18.0.6",
    "@typescript-eslint/eslint-plugin": "^5.36.2",
    "@typescript-eslint/parser": "^5.36.2",
    "@vitejs/plugin-react": "^2.1.0",
    "cross-var": "^1.1.0",
    "dotenv-cli": "^6.0.0",
    "eslint": "^8.23.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.7",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.0",
    "prettier": "^2.7.1",
    "typescript": "^4.6.4",
    "vite": "^3.1.0",
    "vite-tsconfig-paths": "^3.5.0"
  },
  "engines": {
    "node": "^14.0.0"
  }
}
